<%= simple_form_for(@exersize) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :exercise_performed %>
    <%= f.input :load_perscribed %>
    <%= f.input :reps_perscribed %>
    <%= f.input :sets_perscribed %>
    <%= f.input :actual_weight_lifted_1 %>
    <%= f.input :actual_weight_lifted_2 %>
    <%= f.input :actual_weight_lifted_3 %>
    <%= f.association :workout %>
    <%= f.association :user %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
